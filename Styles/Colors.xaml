<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Background colors -->
    <SolidColorBrush x:Key="primaryBackColor" Color="#232234"/>
    <SolidColorBrush x:Key="secondaryBackColor" Color="#2a2b3d"/>
    <!-- Color palette -->
    <SolidColorBrush x:Key="color1" Color="#627eeb"/>
    <SolidColorBrush x:Key="color2" Color="#f39d22"/>
    <SolidColorBrush x:Key="color3" Color="#0bb6ee"/>
    <SolidColorBrush x:Key="color4" Color="#FB539B"/>
    <!-- Font colors -->
    <SolidColorBrush x:Key="textColor1" Color="#fcfefd"/>
    <SolidColorBrush x:Key="textColor2" Color="#b3c9d7"/>
    <!-- Highlight colors -->
    <Color x:Key="highlightColor1">#693dd8</Color>
    <Color x:Key="highlightColor2">#665bf5</Color>
    <!-- Color of change -->
    <SolidColorBrush x:Key="positiveChangeColor" Color="#04d39f"/>
    <SolidColorBrush x:Key="negativeChangeColor" Color="#da5e66"/>
    <!-- Panel colors -->
    <SolidColorBrush x:Key="panelOverColor" Color="#303149"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#3a3b58"/>
    <!-- Ohter colors -->
    <SolidColorBrush x:Key="titleBarColor" Color="#303149"/>

</ResourceDictionary>